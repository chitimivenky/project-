package com.operators;

/*
Arithmetic Operators
..> Addition (+)
..> Subtraction(-)
..> Multiplication(*)
..> Division (/)
..> modulus(%)
Unary Operators
..>Increment(++)
..>Decrement(--)
Relational operators
Conditional Operators
Assignment operators

 */
public class TypeofOperators {
public static void main(String[] args) {
	int a = 20;
	int b = 30;
	System.out.println(a--);
	System.out.println(a);


	
}
}
