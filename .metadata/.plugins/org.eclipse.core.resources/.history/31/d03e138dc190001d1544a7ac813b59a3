package encapsulation;

public class School {

	public static void main(String[] args) {
Student s1=new Student("venkey", 45, 551);
Student s2=new Student("venkew", 455, 550);
Student s3=new Student("venkey=q", 453, 555);
Student s4=new Student("venkey==e", 4525, 575);
 display(s1);
 display(s2);
 display(s3);
 display(s4);
	}

	private static void display(Student s) {
System.out.println(s.name);	
System.out.println(s.id);
System.out.println(s.age);
s.studying();
	}

	}
