public class nnn {
	double totalammount;
	double balance;
	long accountno ;
	double enteramount;
	double debit;
	public void leela() {s
		nnn L = new nnn();
		L.totalammount = 6000;
	}
 public static void main(String[] args) {
	nnn L = new nnn();
	Scanner Sc = new Scanner(System.in);
	 System.out.println("Enter Account Number");
	 L.accountno = Sc.nextLong();
	 
	if (L.accountno == 25463) {
		L.leela();
		System.out.println("Bank name :SBI"+L.totalammount);
	}
	System.out.println("Enter ammount");
	 L.enteramount = Sc.nextDouble();

		 L.debit = L.totalammount - L.enteramount; 
		System.out.println(L.debit+" "+ "is your remaining balance");
